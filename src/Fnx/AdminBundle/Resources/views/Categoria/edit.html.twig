{% extends "FnxAdminBundle::layout.html.twig" %}

{% block title %} Formulário {% endblock %}

{% block menuContent %}
    <div id="menuContent">
        <h3>Categoria Manager: Formulário</h3>
        <ul>
            <li><a class="back-link" href="{{ path('categoriaHome')}}"><span></span>Voltar</a></li>
        </ul> 
    </div>
{% endblock %}

{% block conteudo %}
<div id="box-form">
<form class="cms" action="{{ path('adm_categoria_update', { 'id': entity.id }) }}" method="post" {{ form_enctype(edit_form) }}>
    {{ form_widget(edit_form) }}
        
    <input type="submit" value="Submit" />
</form>
</div>
{% endblock %}

