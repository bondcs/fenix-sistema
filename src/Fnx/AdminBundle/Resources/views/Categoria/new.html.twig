
{% extends "FnxAdminBundle::layout.html.twig" %}

{% block title %} Formulário {% endblock %}

{% block menuContent %}
    <div id="menuContent">
        <h3>Categoria Manager: Formulário</h3>
        <ul>
            <li><a class="back-link" href="{{ path('categoriaHome')}}"><span></span>Voltar</a></li>
        </ul> 
    </div>
{% endblock %}

{% block conteudo %}

<div id="box-form">
    <form class="cms" action="{{ path('adm_categoria_create') }}" method="post" {{ form_enctype(form) }}>
        {{ form_widget(form) }}
        <input type="submit" value="Submit"/>
    </form>
</div>
{% endblock %}
