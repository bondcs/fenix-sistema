    <body>
        <section id="wrapper">
            <header id="header">
                    <div><a href="{{ path('adminHome')}}">Administração</a></div>
                    <div></div>
            </header>
            {% block menu %}
                <div id='cssmenu'>
                    <ul>
                    <li class='has-sub '><a href='{{ path('adminHome')}}'><span>Home</span></a>
                        <ul>
                            <li><a href={{path('_security_change_password_a')}}><span>Alterar Senha</span></a></li>
                        </ul>
                    </li>
                    <li class='has-sub '><a href='{{ path('funcionarioHome')}}'><span>Funcionário</span></a>
                        <ul>
                            <li><a href="{{ path("funcionario_servico_escala")}}">Categoria</a></li>
                            <li><a href="{{ path("escalaPatruIndex")}}">Patrulhamento</a></li>
                            <li><a href="{{ path("escalaFunIndex")}}">Escala</a></li>
                        </ul>
                    </li>
                    <li><a href='{{ path('clienteHome')}}'><span>Cliente</span></a></li>
                    <li><a href='{{ path('atividadeHome')}}'><span>Atividade</span></a></li>
                    <li><a href='{{ path('adm_fornecedor')}}'><span>Fornecedor</span></a></li>
                    <li class='has-sub '><a href='{{path('financeiroShow')}}'><span>Financeiro</span></a>
                        <ul>
                            <li><a href='{{ path('financeiro_instancia')}}'><span>Instância</span></a></li>
                            <li><a href='{{ path('financeiro_conta')}}'><span>Conta</span></a></li>
                            <li><a href="{{ path("salarioHome")}}">Salário</a></li>
                            <li><a href='{{ path('categoriaHome')}}'><span>Categoria</span></a></li>
                            <li><a href='{{ path('financeiro_transferencia')}}'><span>Transferência</span></a></li>
                        </ul>
                    </li>
		    <li><a href='{{path('PedidoListar')}}'><span>Pedido</span></a></li>
		    <li><a href='{% if(app.user.userRoles) %}{{path('chamado')}}{% else %}{{path('chamado_new')}}{% endif %}'><span>Chamados</span></a></li>
                    </ul>
                    <div id="info">
                        <span id="dateInfo">{{ "now"|date("d/m/Y") }}</span>
                        <span class="username">{{ app.user.username }}</span>
                        <a id="logout" href="{{ path('_security_logout_a')}}"><img src="{{ asset('bundles/fnxadmin/images/logout-ico8.png') }}"/></a>
                    </div>
                </div>
            {% endblock %}

            {% block menuContent %}{% endblock %}
            {% if app.session.hasFlash('success') %}
                <div class="flash-success">
                    {{ app.session.flash('success') }}
                </div>
            {% endif %}

            {% if app.session.hasFlash('error') %}
                <div class="flash-error">
                    {{ app.session.flash('error') }}
                    {{ app.session.removeFlash('error') }}
                </div>
            {% endif %}
            {% block subMenu %}{% endblock %}
            <section class="main-col">
                {% block conteudo %}
                    <div id="all-menu">
                        <a href="{{path('atividadeAdd')}}">
                            <div class="box-menu">
                                <div class="img"><img height="60px" src="{{ asset("bundles/fnxadmin/images/note-menu.png")}}" /></div>
                                <div class="label"><span>Atividade</span></div>
                            </div>
                        </a>
                        <a href="{{path('clienteAdd')}}">
                            <div class="box-menu">
                                <div class="img"><img height="60px" src="{{ asset("bundles/fnxadmin/images/cli-menu.png")}}" /></div>
                                <div class="label"><span>Cliente</span></div>
                            </div>
                        </a>
                        <a href="{{path('funcionarioAdd')}}">
                            <div class="box-menu">
                                <div class="img"><img height="60px" src="{{ asset("bundles/fnxadmin/images/worker-menu.png")}}" /></div>
                                <div class="label"><span>Funcionário</span></div>
                            </div>
                        </a>
                        <a href="{{path('calenderHome')}}">
                            <div class="box-menu">
                                <div class="img"><img height="60px" src="{{ asset("bundles/fnxadmin/images/calender-icon.png")}}" /></div>
                                <div class="label"><span>Calendário</span></div>
                            </div>
                        </a>
                        <a href="{{path('PedidoCadastrar')}}">
                            <div class="box-menu">
                                <div class="img"><img height="60px" src="{{ asset("bundles/fnxadmin/images/box4-icon.png")}}" /></div>
                                <div class="label"><span>Pedido</span></div>
                            </div>
                        </a>
                    </div>
                     <div class="profile-index">
                        <div class="ui-panel-content ui-widget-content " style="width:96.5%;">
                            <h4 class="ui-panel-title ui-widget-header" style="text-align: center; padding-top: 1.5%">Informações</h4>
                           <table class="infos">
                               <tr>
                                   <th>Nome</th>
                                   <td>{{funcionario.nome|default('-')}}</td>
                               </tr>
                               <tr>
                                   <th>Telefone</th>
                                   <td>{{funcionario.telefone|default('-')}}</td>
                               </tr>
                               <tr>
                                   <th>Usuário</th>
                                   <td>{{funcionario.usuario.username|default('-')}}</td>
                               </tr>
                               <tr>
                                   <th>Cpf</th>
                                   <td>{{funcionario.cpf|default('-')}}</td>
                               </tr>
                               <tr>
                                   <th>Profile</th>
                                   <td><a class="link-name" href="{{path("funcionarioShow", {"id" : funcionario.id|default('-')})}}">Profile</a></td>
                               </tr>
                           </table>
                        </div>
                        <div>
                            <div id="datepicker" style="margin-top:10px;">
                            </div>
                        </div>
                     </div>

                {% endblock %}
            </section>

            <div id="footer">
                {% block footer %}
                    {% if app.user %}
                    <div id="box-footer">
                        <ul>
